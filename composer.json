{
    "name": "saas-laravel/laravel-plain-sqs",
    "type": "library",
    "description": "Custom SQS connector for Laravel that supports custom format JSON",
    "keywords": [
        "php",
        "laravel",
        "lumen",
        "sqs",
        "aws"
    ],
    "homepage": "https://github.com/saas-laravel/laravel-plain-sqs",
    "license": "MIT",
    "authors": [
        {
            "name": "Przemysław Przyłucki",
            "email": "contact@saaslaravel.com",
            "homepage": "saaslaravel.com"
        },
        {
            "name": "Denis Mysenko",
            "email": "denis@mysenko.com",
            "homepage": "https://www.mysenko.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "^10.0",
        "illuminate/queue": "^10.0",
        "illuminate/bus": "^10.0",
        "aws/aws-sdk-php": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10|^10.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Saaslaravel\\PlainSqs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saaslaravel\\PlainSqs\\Integrations\\LaravelServiceProvider"
            ]
        }
    }
}
