{
    "name": "kibedickson/laravel-plain-sqs",
    "description": "SQS connector for Laravel that supports custom JSON",
    "keywords": [
        "php",
        "laravel",
        "sqs",
        "aws"
    ],
    "homepage": "https://github.com/kibedickson/laravel-plain-sqs",
    "license": "MIT",
    "authors": [
        {
            "name": "Przemysław Przyłucki",
            "email": "contact@saaslaravel.com",
            "homepage": "https://saaslaravel.com"
        },
        {
            "name": "Denis Mysenko",
            "email": "denis@mysenko.com",
            "homepage": "https://www.mysenko.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/queue": "^10.0|^11.0|^12.0",
        "illuminate/bus": "^10.0|^11.0|^12.0",
        "aws/aws-sdk-php": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Saaslaravel\\PlainSqs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saaslaravel\\PlainSqs\\Integrations\\LaravelServiceProvider"
            ]
        }
    }
}
